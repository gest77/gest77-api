{
    "name": "node-js-getting-started",
    "version": "0.3.0",
    "description": "A sample Node.js app using Express 4",
    "engines": {
        "node": "14.x"
    },
    "scripts": {
        "start": "node src/index.js",
        "build": "tsc",
        "deploy": "git push heroku main",
        "postinstall": "tsc",
        "watch-node": "nodemon build/index.js",
        "watch-ts": "tsc -w"
    },
    "main": "index.js",
    "dependencies": {
        "dotenv": "^16.0.1",
        "ejs": "^3.1.5",
        "express": "^4.15.2",
        "typescript": "^4.7.4"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "got": "^11.3.0",
        "tape": "^4.7.0",
        "ts-node": "^10.8.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/heroku/node-js-getting-started"
    },
    "keywords": [
        "node",
        "heroku",
        "express"
    ],
    "license": "MIT"
}
