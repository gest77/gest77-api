{
    "name": "node-js-getting-started",
    "version": "0.3.0",
    "description": "A sample Node.js app using Express 4",
    "engines": {
        "node": "14.x"
    },
    "scripts": {
        "start": "nodemon  ./src/index.ts",
        "build": "tsc",
        "deploy": "git push heroku $(git branch --show-current):main",
        "postinstall": "tsc",
        "watch-node": "nodemon build/index.js",
        "watch-ts": "tsc -w",
        "heroku-start": "yarn ts-node --optimize_for_size --max_old_space_size=460 src/index.ts",
        "lint": "eslint './src/**/*.ts'  --config .eslintrc.json --no-error-on-unmatched-pattern --ignore-path .gitignore ",
        "lintfix": "yarn lint --fix",
        "prettier": "prettier -w ."
    },
    "main": "index.ts",
    "dependencies": {
        "@googleapis/docs": "^1.0.1",
        "@types/app-root-path": "^1.2.4",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.12",
        "@types/cron": "^2.0.0",
        "@types/express": "^4.17.13",
        "@types/express-fileupload": "^1.2.2",
        "@types/i18n": "^0.13.4",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node-fetch": "^2.6.2",
        "@types/nodemailer": "^6.4.4",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^5.30.6",
        "@typescript-eslint/parser": "^5.30.6",
        "app-root-path": "^3.0.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.0.1",
        "ejs": "^3.1.5",
        "eslint": "^8.19.0",
        "express": "^4.15.2",
        "express-fileupload": "^1.4.0",
        "google-spreadsheet": "^3.3.0",
        "googleapis": "^105.0.0",
        "got": "^11.3.0",
        "i18n": "^0.15.0",
        "jsonwebtoken": "^8.5.1",
        "langs": "^2.0.0",
        "log4js": "^6.6.0",
        "nodemon": "^2.0.19",
        "open": "^8.4.0",
        "prettier": "^2.7.1",
        "prisma": "^4.0.0",
        "swagger": "^0.7.5",
        "swagger-jsdoc": "^6.2.1",
        "swagger-json": "^2.0.0",
        "swagger-ui-express": "^4.4.0",
        "tape": "^4.7.0",
        "ts-node": "^10.8.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.7.4",
        "yup": "^0.32.11",
        "yup-env": "^2.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/heroku/node-js-getting-started"
    },
    "keywords": [
        "node",
        "heroku",
        "express"
    ],
    "license": "MIT",
    "devDependencies": {
        "@types/google-spreadsheet": "^3.3.0"
    }
}
